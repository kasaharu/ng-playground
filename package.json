    "@angular/platform-browser": "14.0.6",
    "@angular/platform-browser-dynamic": "14.0.6",
    "@angular/router": "14.0.6",
    "@ngneat/reactive-forms": "5.0.0",
    "@ngneat/tailwind": "7.0.3",
    "@ngrx/component-store": "14.0.2",
    "rxjs": "7.5.6",
    "tslib": "2.3.1",
    "zone.js": "0.11.4"
