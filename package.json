    "angular-cli-ghpages": "1.0.0-rc.2",
    "autoprefixer": "10.3.6",
    "eslint": "^8.18.0",
    "jest": "27.4.5",
    "jest-preset-angular": "11.0.1",
    "msw": "0.35.0",
    "postcss": "8.3.8",
    "prettier": "2.4.1",
